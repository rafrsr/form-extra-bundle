{
  "name": "rafrsr/form-extra-bundle",
  "description": "This bundles provides extra FormType's for Symfony",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Rafael SR",
      "email": "raf.rsr@gmail.com",
      "homepage": "https://github.com/rafrsr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rafrsr\\FormExtraBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rafrsr\\FormExtraBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.4",
    "symfony/framework-bundle": "^3.0",
    "symfony/form": "^3.0",
    "opis/closure": "^2.1",
    "rafrsr/crypto": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.1|^5.1",
    "satooshi/php-coveralls": "dev-master",
    "symfony/doctrine-bridge": "^3.0",
    "doctrine/dbal": "~2.4",
    "doctrine/orm": "~2.4,>=2.4.5"
  },
  "suggest":  {
    "mopa/bootstrap-bundle": "(RECOMMENDED) Add bootstrap style to your forms.",
    "symfony/doctrine-bridge": "Automatically guess form type according to database field type."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
