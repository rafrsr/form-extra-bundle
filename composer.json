{
  "name": "rafrsr/form-extra-bundle",
  "description": "This bundles provides extra FormType's for Symfony",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Rafael SR",
      "email": "raf.rsr@gmail.com",
      "homepage": "https://github.com/rafrsr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rafrsr\\FormExtraBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rafrsr\\FormExtraBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.4",
    "symfony/framework-bundle": "^3.0",
    "symfony/form": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.1|^5.1",
    "satooshi/php-coveralls": "dev-master"
  },
  "suggest":  {
    "mopa/bootstrap-bundle": "^3.0.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
